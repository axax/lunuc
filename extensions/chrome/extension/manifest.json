{
  "manifest_version": 2,
  "name": "Lunuc Extension",
  "description": "Lunuc test chrome extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at":"document_start",
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
