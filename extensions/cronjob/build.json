{
  "types": [
    {
      "name": "CronJob",
      "entryClonable": true,
      "fields": [
        {
          "name": "name",
          "required": true,
          "clone": "${name} (copy)"
        },
        {
          "name": "script",
          "uitype": "jseditor"
        },
        {
          "name": "scriptLanguage",
          "enum": [
            "Javascript",
            "Python"
          ]
        },
        {
          "name": "expression",
          "label": "Cron expression"
        },
        {
          "name": "execfilter",
          "label": "Execution filter"
        },
        {
          "name": "active",
          "type": "Boolean"
        }
      ]
    },
    {
      "name": "CronJobExecution",
      "fields": [
        {
          "name": "state"
        },
        {
          "name": "endTime",
          "type": "Float",
          "uitype": "datetime"
        },
        {
          "name": "scriptLog",
          "uitype": "editor"
        },
        {
          "name": "scriptDebug",
          "uitype": "editor"
        },
        {
          "name": "scriptError",
          "uitype": "editor"
        },
        {
          "name": "cronjob",
          "type": "CronJob"
        }
      ]
    }
  ]
}
